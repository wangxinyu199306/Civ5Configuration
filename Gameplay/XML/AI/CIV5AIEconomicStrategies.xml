		<Column name="WeightThreshold" type="integer" default="0"/>
		<Column name="FirstTurnExecuted" type="integer" default="0"/>
		<Column name="TechPrereq" type="text" reference="Technologies(Type)" default="NULL"/>
		<Column name="TechObsolete" type="text" reference="Technologies(Type)" default="NULL"/>
		<Column name="Advisor" type="text"/>
		<Column name="AdvisorCounsel" type="text"/>
		<Column name="AdvisorCounselImportance" type="integer" default="1"/>
	</Table>
	<Table name="AIEconomicStrategy_Flavors">
		<Column name="AIEconomicStrategyType" type="text" reference="AIEconomicStrategies(Type)"/>
		<Column name="FlavorType" type="text" reference="Flavors(Type)"/>
